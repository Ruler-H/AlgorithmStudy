    "# 백준 11006번 - 남욱이의 닭장\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input().strip())\n",
    "\n",
    "for _ in range(n):\n",
    "    n, m = map(int, input().strip().split())\n",
    "    u = (m * 2) - n\n",
    "    t = m - u\n",
    "\n",
    "    print(f'{u} {t}')"
