    "# 백준 14920번 - 3n+1 수열\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input().strip())\n",
    "\n",
    "cnt = 1\n",
    "\n",
    "while True:\n",
    "    if n == 1:\n",
    "        break\n",
    "    if n % 2 == 0:\n",
    "        n //= 2\n",
    "    else:\n",
    "        n = (n * 3) + 1\n",
    "    cnt += 1\n",
    "\n",
    "print(cnt)"
   ]
