    "# 백준 28014번 - 첨탑 밀어서 부수기\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input().strip())\n",
    "\n",
    "n_list = list(map(int, input().strip().split()))\n",
    "\n",
    "rs = 1\n",
    "be_h = n_list[0]\n",
    "for i in n_list[1:]:\n",
    "    if be_h <= i:\n",
    "        rs += 1\n",
    "    be_h = i\n",
    "\n",
    "print(rs)"
