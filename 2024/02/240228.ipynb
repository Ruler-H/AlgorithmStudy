    "# 백준 2526번 - 싸이클\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n, p = map(int, input().strip().split())\n",
    "\n",
    "arr = [n * n % p]\n",
    "\n",
    "while True:\n",
    "    temp = (arr[-1] * n) % p\n",
    "    if temp in arr:\n",
    "        print(len(arr) - arr.index(temp))\n",
    "        break\n",
    "    arr.append(temp)"
