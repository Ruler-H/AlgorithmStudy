   "source": [
    "# 백준 2885번 - 초콜릿 식사\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "k = int(input())\n",
    "\n",
    "size = 1\n",
    "while k > size:\n",
    "    size *= 2\n",
    "print(size, end=\" \")\n",
    "cnt = 0\n",
    "while True:\n",
    "    if k >= size:\n",
    "        k -= size\n",
    "    if k == 0:\n",
    "        break\n",
    "    size //= 2\n",
    "    cnt += 1\n",
    "print(cnt)"
