   "source": [
    "# 백준 11052번 - 카드 구매하기\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "cards = list(map(int, input().split()))\n",
    "dp = [0] * (N+1)\n",
    "dp[1] = cards[0]\n",
    "\n",
    "for i in range(2, N+1):\n",
    "    for j in range(1, i+1):\n",
    "        dp[i] = max(dp[i], dp[i-j] + cards[j-1])\n",
    "\n",
    "print(dp[N])"
   ]
  }
