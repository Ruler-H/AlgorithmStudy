    "# 백준 1351번 - 무한 수열\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N, P, Q = map(int, input().split())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "dp = dict()\n",
    "dp[0] = 1\n",
    "\n",
    "def solve(n):\n",
    "    if n in dp:\n",
    "        return dp[n]\n",
    "    else:\n",
    "        dp[n] = solve(n//P) + solve(n//Q)\n",
    "        return dp[n]\n",
    "    \n",
    "print(solve(N))"
   ]
  },
