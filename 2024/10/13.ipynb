    "# 백준 2133번 - 타일 채우기\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "sys.setrecursionlimit(10**6)\n",
    "\n",
    "N = int(input())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "dp = [0] * 31\n",
    "dp[0] = 1\n",
    "dp[2] = 3\n",
    "\n",
    "for i in range(4, N+1, 2):\n",
    "    dp[i] = dp[i-2] * dp[2]\n",
    "    for j in range(4, i+1, 2):\n",
    "        dp[i] += dp[i-j] * 2\n",
    "print(dp[N])"
   ]
  },
