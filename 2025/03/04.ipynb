    "# 백준 6996번 - 애너그램\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "sys.setrecursionlimit(10**6)\n",
    "\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    a, b = input().split()\n",
    "    if sorted(a) == sorted(b):\n",
    "        print(a, \"&\", b, \"are anagrams.\")\n",
    "    else:\n",
    "        print(a, \"&\", b, \"are NOT anagrams.\")"
