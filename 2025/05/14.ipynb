    "# 백준 4641번 - Doubles\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "while True:\n",
    "    inp = input().strip()\n",
    "    if inp == '-1':\n",
    "        break\n",
    "    nums = list(map(int, inp.split()))[:-1]\n",
    "    cnt = 0\n",
    "    for i in nums:\n",
    "        if i * 2 in nums:\n",
    "            cnt += 1\n",
    "    print(cnt)"
   ]
