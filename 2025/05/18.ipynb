    "# 백준 8723번 - Patyki\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "a, b, c = sorted(map(int, input().split()))\n",
    "\n",
    "# 삼각형이 아예 안 되는 경우\n",
    "if a + b <= c:\n",
    "    print(0)\n",
    "# 정삼각형\n",
    "elif a == b == c:\n",
    "    print(2)\n",
    "# 직각삼각형\n",
    "elif a * a + b * b == c * c:\n",
    "    print(1)\n",
    "# 둘 다 아님\n",
    "else:\n",
    "    print(0)\n"
