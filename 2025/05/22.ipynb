    "# 백준 9713번 - Sum of Odd Sequence\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "T = int(input())\n",
    "for _ in range(T):\n",
    "    N = int(input())\n",
    "    ans = 0\n",
    "    for i in range(1, N + 1, 2):\n",
    "        ans += i\n",
    "    print(ans)"
