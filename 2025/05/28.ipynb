    "# 백준 8710번 - Koszykarz\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "k, w, m = map(int, input().split())\n",
    "\n",
    "if k >= w:\n",
    "    print(0)\n",
    "else:\n",
    "    need_feet = w - k\n",
    "    years = need_feet // m\n",
    "    if need_feet % m != 0:\n",
    "        years += 1\n",
    "    print(years)"
