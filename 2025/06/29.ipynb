    "# 백준 15025번 - Judging Moose\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "e, r = map(int, input().split())\n",
    "\n",
    "if e == 0 and r == 0:\n",
    "    print(\"Not a moose\")\n",
    "elif e == r:\n",
    "    print(\"Even\", e + r)\n",
    "else:\n",
    "    print(\"Odd\", 2 * max(e, r))\n"
   ]
