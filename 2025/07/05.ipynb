    "# 백준 4696번 - St. Ives\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "while True:\n",
    "    n = float(input())\n",
    "    if n == 0:\n",
    "        break\n",
    "    result = 1\n",
    "    result += n\n",
    "    result += (n ** 2)\n",
    "    result += (n ** 3)\n",
    "    result += (n ** 4)\n",
    "    print(f\"{result:.2f}\")"
   ]
