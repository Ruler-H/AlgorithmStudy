    "# 백준 31962번 - 등교\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N, X = map(int, input().split())\n",
    "result = -1\n",
    "for i in range(N):\n",
    "    S, T = map(int, input().split())\n",
    "    if S + T <= X:\n",
    "        result = max(result, S)\n",
    "\n",
    "print(result)"
