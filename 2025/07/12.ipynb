    "# 백준 18414번 - X に最も近い値 (The Nearest Value)\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "X, L, R = map(int, input().split())\n",
    "\n",
    "if L <= X <= R:\n",
    "    print(X)\n",
    "elif X < L:\n",
    "    print(L)\n",
    "else:\n",
    "    print(R)"
