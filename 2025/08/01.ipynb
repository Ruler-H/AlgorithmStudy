    "# 백준 25400번 - 제자리\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "cnt = 0\n",
    "target = 1\n",
    "for i in range(N):\n",
    "    if arr[i] == target:\n",
    "        target += 1\n",
    "    else:\n",
    "        cnt += 1\n",
    "print(cnt)"
