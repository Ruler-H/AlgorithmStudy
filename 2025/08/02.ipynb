    "# 백준 30868번 - 개표\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    n = int(input())\n",
    "    result = [\"++++\" for _ in range(n // 5)]\n",
    "    result.append(\"|\" * (n % 5))\n",
    "    print(\" \".join(result))"
