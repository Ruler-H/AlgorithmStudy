    "# 백준 15128번 - Congruent Numbers\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "p1, q1, p2, q2 = map(int, input().split())\n",
    "\n",
    "molecule = p1 * p2\n",
    "denominator = q1 * q2 * 2\n",
    "\n",
    "if molecule % denominator == 0:\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
   ]
