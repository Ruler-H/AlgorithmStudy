   "source": [
    "# 백준 31876번 - 홀짝홀짝\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "K = input()\n",
    "cnt = 0\n",
    "for i in range(N):\n",
    "    if int(K[i]) % 2 == 0:\n",
    "        cnt += 1\n",
    "    else:\n",
    "        cnt -= 1\n",
    "\n",
    "if cnt > 0:\n",
    "    print(0)\n",
    "elif cnt < 0:\n",
    "    print(1)\n",
    "else:\n",
    "    print(-1)"
   ]
  }
