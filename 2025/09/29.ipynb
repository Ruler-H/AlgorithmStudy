    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "A.sort()\n",
    "\n",
    "for i in range(N):\n",
    "    if A[i] != i + 1:\n",
    "        print(i + 1)\n",
    "        sys.exit()\n",
    "print(N+1)"
