    "# 백준 24087번 - アイスクリーム (Ice Cream)\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "S = int(input())\n",
    "A = int(input())\n",
    "C = int(input())\n",
    "\n",
    "result = 250\n",
    "S -= A\n",
    "\n",
    "if S > 0:\n",
    "    result += (100 * (S // C))\n",
    "    if S % C != 0:\n",
    "        result += 100\n",
    "\n",
    "print(result)"
   ]
