    "# 백준 18698번 - The Walking Adam\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "t = int(input())\n",
    "\n",
    "for _ in range(t):\n",
    "    s = input().strip()\n",
    "    if 'D' in s:\n",
    "        print(s.index('D'))  # 'D'가 나타나는 위치 = 그 전까지의 'U' 개수\n",
    "    else:\n",
    "        print(len(s))\n"
