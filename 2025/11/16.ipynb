    "# 백준 26546번 - Reverse\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "for _ in range(n):\n",
    "    s, i, j = input().split()\n",
    "    i = int(i)\n",
    "    j = int(j)\n",
    "    print(s[:i] + s[j:])"
