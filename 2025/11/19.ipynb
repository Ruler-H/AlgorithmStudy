    "# 백준 18198번 - Basketball One-on-One\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "inp_str = input()\n",
    "\n",
    "a = 0\n",
    "b = 0\n",
    "for i in range(len(inp_str)):\n",
    "    if inp_str[i] == 'A':\n",
    "        a += 1\n",
    "    elif inp_str[i] == 'B':\n",
    "        b += 1\n",
    "\n",
    "if a > b:\n",
    "    print(\"A\")\n",
    "else:\n",
    "    print(\"B\")"
   ]
