    "# 백준 33964번 - 레퓨닛의 덧셈\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "X, Y = map(int, input().split())\n",
    "\n",
    "result = \"\"\n",
    "\n",
    "for i in range(max(X, Y), 0, -1):\n",
    "    result += str((i <= X) + (i <= Y))\n",
    "\n",
    "print(result)"
