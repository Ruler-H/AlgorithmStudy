    "# 백준 32642번 - 당구 좀 치자 제발\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "weather = list(map(int, input().split()))\n",
    "anger = 0\n",
    "result = 0\n",
    "for i in range(N):\n",
    "    if weather[i] == 1:\n",
    "        anger += 1\n",
    "    else:\n",
    "        anger -= 1\n",
    "    result += anger\n",
    "print(result)"
