    "# 백준 25828번 - Corona Virus Testing\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "g, p, t = map(int, input().split())\n",
    "\n",
    "individual = g * p          # 개별 테스트\n",
    "group = g + (t * p)         # 그룹 테스트\n",
    "\n",
    "if individual < group:\n",
    "    print(1)\n",
    "elif individual > group:\n",
    "    print(2)\n",
    "else:\n",
    "    print(0)"
