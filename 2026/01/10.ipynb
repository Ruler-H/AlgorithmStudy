    "# 백준 14782번 - Bedtime Reading, I\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "i = int(input())\n",
    "\n",
    "result = 0\n",
    "for j in range(1, int(i ** 0.5) + 1):\n",
    "    if i % j == 0:\n",
    "        result += j\n",
    "        result += i // j\n",
    "\n",
    "print(result)"
