    "# 백준 32068번 - 보물 찾기\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    L, R, S = map(int, input().split())\n",
    "    L_step = 2 * (S - L) + 1  # 홀수 단계\n",
    "    R_step = 2 * (R - S)      # 짝수 단계\n",
    "    print(min(L_step, R_step))"
   ]
