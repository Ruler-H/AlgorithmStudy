    "# 백준 19564번 - 반복\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "S = input().strip()\n",
    "\n",
    "result = 1\n",
    "word = 96\n",
    "for i in range(len(S)):\n",
    "    if ord(S[i]) > word:\n",
    "        word = ord(S[i])\n",
    "    else:\n",
    "        result += 1\n",
    "        word = ord(S[i])\n",
    "\n",
    "print(result)"
   ]
