    "import sys\n",
    "\n",
    "for line in sys.stdin:\n",
    "    prev = None\n",
    "    while prev != line:\n",
    "        prev = line\n",
    "        line = line.replace(\"BUG\", \"\")\n",
    "    print(line, end=\"\")"
