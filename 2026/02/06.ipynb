   "source": [
    "# 백준 27880번 - Gahui and Soongsil University station\n",
    "import sys\n",
    "\n",
    "infos = sys.stdin.readlines()\n",
    "\n",
    "total = 0\n",
    "for info in infos:\n",
    "    tool, cm = info.split()\n",
    "    if tool == \"Es\":\n",
    "        total += 21 * int(cm)\n",
    "    else:\n",
    "        total += 17 * int(cm)\n",
    "\n",
    "print(total)"
   ]
