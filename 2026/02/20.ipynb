    "# 백준 14682번 - Shifty Sum\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "k = int(input())\n",
    "\n",
    "result = 0\n",
    "for i in range(k + 1):\n",
    "    result += N * (10 ** i)\n",
    "\n",
    "print(result)"
