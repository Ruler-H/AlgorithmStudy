    "# 백준 14723번 - 이산수학 과제\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "# N이 몇 번째 대각선에 속하는지 찾기\n",
    "k = 1\n",
    "while k * (k + 1) // 2 < N:\n",
    "    k += 1\n",
    "\n",
    "# 대각선 내에서의 위치\n",
    "pos = N - (k - 1) * k // 2\n",
    "\n",
    "# 분자: k+1-pos, 분모: pos\n",
    "a = k + 1 - pos\n",
    "b = pos\n",
    "\n",
    "print(a, b)\n",
    "\n"
   ]
