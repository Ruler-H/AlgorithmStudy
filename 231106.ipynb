    "# 백준 5523번 - 경기 결과\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "n = int(input().strip())\n",
    "\n",
    "a_win = 0\n",
    "b_win = 0\n",
    "\n",
    "for _ in range(n):\n",
    "    a, b = map(int, input().strip().split())\n",
    "    if a > b:\n",
    "        a_win += 1\n",
    "    elif a < b:\n",
    "        b_win += 1\n",
    "\n",
    "print(f'{a_win} {b_win}')"
   ]
