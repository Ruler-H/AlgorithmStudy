    "# 백준 16673번 - 고려대학교에는 공식 와인이 있다\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "c, k, p = map(int, input().strip().split())\n",
    "\n",
    "result = sum(range(1, c+1)) * k\n",
    "result += sum(map(lambda x: x ** 2,range(1, c+1))) * p\n",
    "print(result)"
