    "# 백준 14913번 - 등차수열에서 항 번호 찾기\n",
    "import sys\n",
    "# input = sys.stdin.readline\n",
    "\n",
    "a, d, k = map(int, input().strip().split())\n",
    "\n",
    "n = ((k-a) / d) + 1\n",
    "\n",
    "if int(n) == n:\n",
    "    if n <= 0:\n",
    "        print('X')\n",
    "    else:\n",
    "        print(int(n))\n",
    "else:\n",
    "    print('X')"
